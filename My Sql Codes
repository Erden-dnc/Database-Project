-- Tablo için indeksler

ALTER TABLE `Advisor`
  ADD PRIMARY KEY (`advisor_id`),
  ADD KEY `IX_Advisor_Last_Name` (`last_name`);


ALTER TABLE `Author`
  ADD PRIMARY KEY (`author_id`),
  ADD KEY `IX_Author_Last_Name` (`last_name`);


ALTER TABLE `Institute`
  ADD PRIMARY KEY (`institute_id`),
  ADD UNIQUE KEY `UQ_Institute_Name_University` (`institute_name`,`university_id`),
  ADD KEY `IX_Institute_University` (`university_id`);


ALTER TABLE `Keyword`
  ADD PRIMARY KEY (`keyword_id`),
  ADD UNIQUE KEY `UQ_Keyword` (`word`);


ALTER TABLE `Thesis`
  ADD PRIMARY KEY (`thesis_id`),
  ADD UNIQUE KEY `UQ_Thesis_Number` (`thesis_number`),
  ADD KEY `IX_Thesis_Author` (`author_id`),
  ADD KEY `IX_Thesis_University` (`university_id`),
  ADD KEY `IX_Thesis_Institute` (`institute_id`),
  ADD KEY `IX_Thesis_Year` (`year`),
  ADD KEY `IX_Thesis_Type` (`type`),
  ADD KEY `IX_Thesis_Language` (`language`),
  ADD KEY `IX_Thesis_Title` (`title`);


ALTER TABLE `Thesis_Advisor`
  ADD PRIMARY KEY (`thesis_advisor_id`),
  ADD UNIQUE KEY `UQ_Thesis_Advisor_Role` (`thesis_id`,`advisor_id`,`role`),
  ADD KEY `IX_Thesis_Advisor_Thesis` (`thesis_id`),
  ADD KEY `IX_Thesis_Advisor_Advisor` (`advisor_id`);


ALTER TABLE `Thesis_Keyword`
  ADD PRIMARY KEY (`thesis_keyword_id`),
  ADD UNIQUE KEY `UQ_Thesis_Keyword` (`thesis_id`,`keyword_id`),
  ADD KEY `IX_Thesis_Keyword_Thesis` (`thesis_id`),
  ADD KEY `IX_Thesis_Keyword_Keyword` (`keyword_id`);


ALTER TABLE `Thesis_Topic`
  ADD PRIMARY KEY (`thesis_topic_id`),
  ADD UNIQUE KEY `UQ_Thesis_Topic` (`thesis_id`,`topic_id`),
  ADD KEY `IX_Thesis_Topic_Thesis` (`thesis_id`),
  ADD KEY `IX_Thesis_Topic_Topic` (`topic_id`);


ALTER TABLE `Topic`
  ADD PRIMARY KEY (`topic_id`),
  ADD UNIQUE KEY `UQ_Topic_Name` (`topic_name`);


ALTER TABLE `University`
  ADD PRIMARY KEY (`university_id`),
  ADD UNIQUE KEY `UQ_University_Name` (`university_name`);

-- Dökümü yapılmış tablolar için AUTO_INCREMENT değeri


ALTER TABLE `Advisor`
  MODIFY `advisor_id` int(7) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=8;

ALTER TABLE `Author`
  MODIFY `author_id` int(7) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=8;

ALTER TABLE `Institute`
  MODIFY `institute_id` int(7) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=8;

ALTER TABLE `Keyword`
  MODIFY `keyword_id` int(7) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=16;

ALTER TABLE `Thesis`
  MODIFY `thesis_id` int(7) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=8;

ALTER TABLE `Thesis_Advisor`
  MODIFY `thesis_advisor_id` int(7) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=10;

ALTER TABLE `Thesis_Keyword`
  MODIFY `thesis_keyword_id` int(7) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=19;

ALTER TABLE `Thesis_Topic`
  MODIFY `thesis_topic_id` int(7) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=20;

ALTER TABLE `Topic`
  MODIFY `topic_id` int(7) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=16;

ALTER TABLE `University`
  MODIFY `university_id` int(7) NOT NULL AUTO_INCREMENT, AUTO_INCREMENT=7;

-- Tablo kısıtlamaları 

ALTER TABLE `Institute`
  ADD CONSTRAINT `FK_Institute_University` FOREIGN KEY (`university_id`) REFERENCES `University` (`university_id`) ON DELETE CASCADE;


ALTER TABLE `Thesis`
  ADD CONSTRAINT `FK_Thesis_Author` FOREIGN KEY (`author_id`) REFERENCES `Author` (`author_id`) ON DELETE NO ACTION,
  ADD CONSTRAINT `FK_Thesis_Institute` FOREIGN KEY (`institute_id`) REFERENCES `Institute` (`institute_id`) ON DELETE NO ACTION,
  ADD CONSTRAINT `FK_Thesis_University` FOREIGN KEY (`university_id`) REFERENCES `University` (`university_id`) ON DELETE NO ACTION;


ALTER TABLE `Thesis_Advisor`
  ADD CONSTRAINT `FK_Thesis_Advisor_Advisor` FOREIGN KEY (`advisor_id`) REFERENCES `Advisor` (`advisor_id`) ON DELETE NO ACTION,
  ADD CONSTRAINT `FK_Thesis_Advisor_Thesis` FOREIGN KEY (`thesis_id`) REFERENCES `Thesis` (`thesis_id`) ON DELETE CASCADE;


ALTER TABLE `Thesis_Keyword`
  ADD CONSTRAINT `FK_Thesis_Keyword_Keyword` FOREIGN KEY (`keyword_id`) REFERENCES `Keyword` (`keyword_id`) ON DELETE NO ACTION,
  ADD CONSTRAINT `FK_Thesis_Keyword_Thesis` FOREIGN KEY (`thesis_id`) REFERENCES `Thesis` (`thesis_id`) ON DELETE CASCADE;


ALTER TABLE `Thesis_Topic`
  ADD CONSTRAINT `FK_Thesis_Topic_Thesis` FOREIGN KEY (`thesis_id`) REFERENCES `Thesis` (`thesis_id`) ON DELETE CASCADE,
  ADD CONSTRAINT `FK_Thesis_Topic_Topic` FOREIGN KEY (`topic_id`) REFERENCES `Topic` (`topic_id`) ON DELETE NO ACTION;
COMMIT;
