CREATE TABLE `Thesis` (
  `thesis_id` int(7) NOT NULL,
  `thesis_number` int(7) NOT NULL,
  `title` varchar(500) NOT NULL,
  `abstract` varchar(5000) NOT NULL,
  `author_id` int(7) NOT NULL,
  `year` int(7) NOT NULL,
  `type` varchar(5000) NOT NULL,
  `university_id` int(7) NOT NULL,
  `institute_id` int(7) NOT NULL,
  `page_count` int(7) DEFAULT NULL,
  `language` varchar(50) NOT NULL,
  `submission_date` date NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;


